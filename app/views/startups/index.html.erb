<div class="container">
  <div class="sidebar">
  </div>
  <div class="list_startups">
    <div class="title">
      <h1> Aristotle </h1>
      <%= link_to "Download Logos", download_logos_path %>
    </div>

    <% @startups.each do |startup| %>
      <div class="startup">
        <img src="<%= "https://images.crunchbase.com/image/upload/c_lpad,f_auto,q_auto:eco,dpr_1/#{startup[:logo]}" %>" class="logo">
        <h2><%= startup[:name] %></h2>
      </div>
    <% end %>
  </div>
</div>
